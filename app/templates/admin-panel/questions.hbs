<div class="inner-width">
	<h2 class="selection-title">Питання користувачів</h2>
	<div class="border"></div>
</div>

<div class="question-selection">
	{{#each model as |question|}}
		{{ui-questions
			FIO=question.FIO
			emailAdress=question.emailAdress
			mobileNumber=question.mobileNumber
			message=question.message
			created=question.created
			dateQuestion=question.dateQuestion
		}}
	{{/each}}
</div>